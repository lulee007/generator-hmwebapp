{
    "extends": [
        "eslint:recommended",
        "angular"
    ],
    "globals": {
        "HMAMap": true,
        "HandsMap":true,
        "ol":true,
        "AMap": true,
        "hmgeolocation": true,
        "Howl": true,
        "VConsole": true,
        "Terraformer": true,
        "$":true
    },
    "env": {
        "browser": true
    },
  
    "rules": {
      "angular/di": 0,
        "wrap-iife": [
            2,
            "inside"
        ],
        "eqeqeq": 2,
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "no-unused-vars": [
            2,
            {
                "vars": "local",
                "args": "none"
            }
        ],
        "no-multi-str": 2,
        "no-irregular-whitespace": 2,
        "semi": [
            2,
            "always"
        ],
        "indent": 2,
        "angular/log": 0,
        "no-console": 0,
        "linebreak-style": 0,
        "eol-last": 2
    }
  }
  